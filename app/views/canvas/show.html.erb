<article class='canvas'>
  <h2>Hello <fb:name uid='<%= current_user.identifier %>' useyou='false'></fb:name>!</h2>
</article>

<% unless friends_on_app.nil? %>
  <% friends_on_app.each do |friend_id| %>
    <div><fb:name uid='<%= friend_id %>' useyou='false'></fb:name></div>
  <% end %>
<% end %>